{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noFallthroughCasesInSwitch": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "target": "ES2015"
    },
    "files": [
        "main.ts",
        "Token.ts",
        "Scanner.ts",
        "types.ts",
        "AstPrinter.ts",
        "Parser.ts"
    ]
}
